<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fy.baselibrary.test" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="26" />

    <instrumentation
        android:name="android.support.test.runner.AndroidJUnitRunner"
        android:functionalTest="false"
        android:handleProfiling="false"
        android:label="Tests for com.fy.baselibrary.test"
        android:targetPackage="com.fy.baselibrary.test" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:label="@string/app_name"
        android:supportsRtl="true" >
        <uses-library android:name="android.test.runner" />
        <!-- 欢迎页 -->
        <activity
            android:name="com.fy.baselibrary.startactivity.StartUpActivity"
            android:label="@string/srartUpTitle"
            android:screenOrientation="portrait" /> <!-- 全局 权限管理 -->
        <activity
            android:name="com.fy.baselibrary.permission.PermissionActivity"
            android:theme="@style/myTransparent" />
    </application>

</manifest>